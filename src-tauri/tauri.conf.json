{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "mia",
    "version": "1.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "copyright": "Azuyamat",
      "targets": "all",
      "identifier": "com.azuyamat.mia",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/icon.ico",
        "icons/icon.png"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Mia",
        "width": 1000,
        "height": 800
      }
    ],
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/AzuyaMat/mia_gui/releases/latest/download/latest.json"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQwQUJBNkRFRThBQUVEREYKUldUZjdhcm8zcWFyMEZZRVNBb1RTUDhhekNwcFBObmJjTy96SWMvSnRzQ3I3S2lTczBXSzBmWWIK",
      "windows": {
        "installMode": "passive"
      }
    }
  }
}
